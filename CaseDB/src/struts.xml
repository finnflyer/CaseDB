<struts>
	<package name="user" extends="struts-default" namespace="/user">
		<action name="logIn" class="loginAction">
			<result name="success"  type="redirectAction">
   				<param name="actionName">Home</param>  
          		  <param name="namespace">/phase4</param>  
			</result>
			<result name="error">/jsp/LogError.jsp</result>			
		</action>
		<action name ="regsiter" class="registerAction">
			<result name="success" >/index.jsp</result>
			<result name="error">/jsp/RegisterError.jsp</result>
		</action>
			<action name="SearchRole" class="SearchRole">
					<result name="success">/jsp/user/RoleHome.jsp</result>
			</action>
			<action name="EditUserRole" class="EditRole">
				<result name="success">/jsp/user/EditRole.jsp</result>
			</action>
				<action name="UpdateRole" class="UpdateRole">
				<result name="success">/jsp/user/RoleHome.jsp</result>
			</action>
		<action name="Logout" class="logoutAction">
			<result name="success">/index.jsp</result>
		</action>
	</package>
	<package name="downFile" extends="struts-default" namespace="/download">
		<action name="getPictureForAjax" class="ShowPicture"></action>
	</package>
<package name="phase4" extends="struts-default" namespace="/phase4">

	<!--  	<global-results>
			<result name="exceptionPage" >/common/error2.jsp</result>
			<result name="logError">/index.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="exceptionPage" name="exceptionPage" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings> -->
		<action name="Help">
		<result  name="success">/jsp/Help.jsp</result>
		</action>
	  <action name="Home" class="Home">
			<result name="success">/jsp/Home.jsp</result>
			<result name="error">/jsp/common/error.jsp</result>
		</action>		
		<action name="TestCaseHome" class="pressHome">
			<result name="success">SearchTestCase?searchflag=1</result>
		</action>
		<action name="StepOne" class="StepOne">
			<result name="success">/jsp/TestCases/StepOne.jsp</result>
		</action>
		<action name="StepTwo" class="SaveStepOne">
		  <result name="success">/jsp/TestCases/StepTwo.jsp</result>
		  <result name="input">/jsp/TestCases/StepOne.jsp</result>
		</action>
		<action name="StepFinal" class="SaveStepTwo">
			<result name="success">/jsp/TestCases/StepThree.jsp</result>
			<result name="input" >/jsp/TestCases/StepTwo.jsp</result>
		</action>
		<action name="SaveFinal" class="SaveFinal">
			<result name="success">/jsp/TestCases/TCHome.jsp</result>
		</action>
		<action name="SearchTestCase" class="SearchCase">
			<result name="success">/jsp/TestCases/TCHome.jsp</result>
		</action>
		<action name="ShowTestCaseDetail" class="ShowTestCaseDetail">
			<result name="success">/jsp/TestCases/showTCInfoViewDetails.jsp</result>
		</action>
		<action name="EditCaseStepOne" class="EditCaseStepOne">
			<result name="success">/jsp/TestCases/EditTestCaseStepOne.jsp</result>
		</action>
		<action name="UpdateCaseStepone" class="UpdateCaseStepone">
			<result name="success">/jsp/TestCases/EditTestCaseStepTwo.jsp</result>
		</action>
		<action name="UpdateCaseStepTwo" class="UpdateCaseStepTwo">
			<result name="success">/jsp/TestCases/EditTestCaseStepFinal.jsp</result>
		</action>
		<action name="UpdateCaseStepFinal" class="UpdateCaseStepFinal">
			<result name="success">/jsp/TestCases/TCHome.jsp</result>
		</action>
		<action name="DeleteCase" class="DeleteCase">
			<result name="success">SearchTestCase?searchflag=1</result>
		</action>
		<action name="DeletePhoto" class="DeletePhoto">
		</action>
		
		
	<!-- =============================TestPlan Part====================================  -->
	 <action name="TestPlanHome" class="TestPlanHome">
	 	<result name="success">SearchTestPlan?searchflag=1</result>
	 </action>
	 <action name="preTestPlan" class="preTestPlan">
	 	<result name="success">/jsp/TestPlan/SearchTestCaseForPlan.jsp</result>
	 </action>
	 <action name="SearchTestCaseForPlan" class="SearchTestCaseForPlan">
	 	<result name="success">/jsp/TestPlan/SearchTestCaseForPlan.jsp</result>
	 </action>
	 <action name="SaveContentToNewPlan" class="SaveContentToNewPlan">
	 </action>
	 <action name="ShowTestPlan" class="ShowTestPlan">
	 	<result name="success">/jsp/TestPlan/ShowTestPlan.jsp</result>
	 </action>
	 <action name="EditTestPlan" class="EditTestPlan">
	 	<result name="success">/jsp/TestPlan/EditTestPlan.jsp</result>
	 </action>
	 <action name="SearchTestPlan" class="SearchForPlan">
	 	<result name="success">/jsp/TestPlan/TPHome.jsp</result>
	 </action>
	 <action name="SaveTestPlanAction" class="SaveTestPlanAction"></action>
	<action name="UpdateTestPlan" class="UpdateTestPlan">
		<result name="success">/jsp/TestPlan/TPHome.jsp</result>
	</action>
	<action name="SaveContentToOlderTestPlan" class="SaveContentToOlderTestPlan"></action>
	<action name="DeleteTestPlan" class="DeleteTestPlan">
		<result name="success">/jsp/TestPlan/TPHome.jsp</result>
	</action>

	<!-- =============================TestTool Part====================================  -->
	<action name="TestToolHome" >
		<result name="success">/jsp/TestTools/TestToolHome.jsp</result>
	</action>
	<action name="preSaveFile">
		<result  name="success">/jsp/TestTools/UploadFileProperty.jsp</result>
	</action>
	<action name="SaveFile" class="preSaveFile">
		<result  name="success">/jsp/TestTools/UploadFileDetail.jsp</result>
	</action>
	<action name="preUploadFile" class="preUploadFile">
		<result  name="success">/jsp/TestTools/UploadFileDetail.jsp</result>
	</action>
	<action name="uploadFile" class="uploadFile"></action>
	<action name="deleteFile" class="deleteFile">
		<result name="success">/jsp/TestTools/TestToolHome.jsp</result>
	</action>
	<action name="SearchToolForPage" class="SearchToolForPage">
		<result name="success">/jsp/TestTools/TestToolHome.jsp</result>
	</action>
	<action name="ShowToolDetail" class="ShowToolDetail">
		<result name="success">/jsp/TestTools/ViewFileDetail.jsp</result>
	</action>
	<action name="downloadFileAction" class="downloadFile">
			<result type="stream">
				<param name="contentDisposition">attachment;filename=${toolBean.uploadFileName}</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
</package>

</struts>