<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lenovo.ittools.ctd.bean.testcase.CaseLanMap"  table="ctdlangmap">
		<id name="languageID" column="languagekey" type="integer">
			<generator class="increment"></generator>
		</id>
		<property name="caseInstkey"  type="string"></property>
	    <property name="lanValue"  type="string"></property>
		<property name="osDes"  type="integer"></property>
		<property name="localSet"  type="integer"></property>		
	</class>
</hibernate-mapping>