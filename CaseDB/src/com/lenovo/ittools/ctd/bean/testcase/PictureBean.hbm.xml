<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lenovo.ittools.ctd.bean.testcase.PictureBean" table="ctdcasepicture">
		<id name="pictureInstkey" column="pictureinstkey" type="string">
			<generator class="assigned"></generator>
		</id>
		<property name="type"  type="integer"></property>	
		<property name="filePath"  type="string"></property>
		<property name="caseInstkey"  type="string"></property>		
		<property name="caseContentInstkey"  type="string"></property>
		<property name="fileName"  type="string"></property>
		<property name="createDate" type="timestamp"></property>
	</class>
</hibernate-mapping>