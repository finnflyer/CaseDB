<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lenovo.ittools.ctd.bean.testplan.TestPlanBean" table="ctdtestplan">
		<id name="testPlanInstkey" column="testPlanInstkey" type="string">
			<generator class="assigned"></generator>
		</id>
		<property name="testPlanName"  type="string"></property>
		<property name="testPlanType" type="string"></property>
		<property name="projectInstkey"  type="string"></property>
		<property name="projectName"  type="string"></property>
		<property name="testPlanOwnerInstkey"  type="string"></property>
		<property name="testPlanOwner" type="string"></property>
		<property name="createDate"  type="timestamp"></property>
		<property name="testPlanDescription"  type="string"></property>
		<property name="testPlanStatus"  type="string"></property>
		<property name="testPlanComments"  type="string"></property>

	</class>
</hibernate-mapping>